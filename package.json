{
  "name": "@amateour/middleware-sandwich",
  "version": "0.1.1",
  "description": "Integrated connection middleware, allows to improve the traceability of the arguments and parameters transmitted by HTTP request.",
  "main": "build/index.js",
  "module": "build/index.es.js",
  "jsnext:main": "build/index.es.js",
  "typings": "build/index.d.ts",
  "type": "module",
  "libCustom": {
    "validator": {
      "main": "build/validator.js",
      "module": "build/validator.es.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rollup -c",
    "test": "jest",
    "test-coverage": "jest --coverage && coveralls < coverage/lcov.info",
    "lint": "lint-staged",
    "prepare": "husky install",
    "express:dev": "nodemon --watch './**/*.ts' --exec 'node --experimental-specifier-resolution=node --loader ts-node/esm' express.ts"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "repository": "git@github.com:amateour/middleware-sandwich.git",
  "author": "Brayan Salgado <binariado@gmail.com>",
  "license": "GPL 3.0",
  "keywords": [],
  "files": [
    "build",
    "functions",
    "index.js"
  ],
  "devDependencies": {
    "@babel/preset-env": "^7.15.8",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.178",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "babel-jest": "^27.3.1",
    "body-parser": "^1.19.0",
    "eslint": "^8.1.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "express": "^4.17.1",
    "husky": "^7.0.0",
    "jest": "^27.3.1",
    "lint-staged": "^11.2.3",
    "nodemon": "^2.0.14",
    "rollup": "^2.58.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  }
}
